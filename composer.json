{
    "name": "henrotaym/laravel-model-queries",
    "description": "Move your queries from your models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-test-suite": "^3.0",
        "henrotaym/laravel-container-auto-register": "^2.0",
        "henrotaym/laravel-package-versioning-config": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelModelQueries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelModelQueries\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelModelQueries\\Providers\\LaravelModelQueriesServiceProvider"
            ]
        }
    }
}
